{
  "name": "ink-whale-be",
  "version": "0.0.1",
  "description": "InkWhale_Backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t ink-whale-be .",
    "docker:run": "docker run -p 3000:3000 -d ink-whale-be",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "seteda54 <tungnt.hust.edu@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^5.0.3",
    "@loopback/core": "^4.0.3",
    "@loopback/cron": "^0.9.7",
    "@loopback/repository": "^5.1.2",
    "@loopback/rest": "^12.0.3",
    "@loopback/rest-explorer": "^5.0.3",
    "@loopback/service-proxy": "^5.0.3",
    "@polkadot/api": "^9.13.6",
    "@polkadot/api-contract": "^9.13.6",
    "@polkadot/keyring": "^10.3.1",
    "@polkadot/types": "^9.13.6",
    "@polkadot/util": "^10.3.1",
    "@types/file-type": "^10.9.1",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/numeral": "^2.0.5",
    "axios": "^1.3.2",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.3",
    "dotenv-extended": "^2.9.0",
    "ethers": "^6.9.2",
    "file-type": "^16.5.3",
    "fs": "^0.0.1-security",
    "ipfs-http-client": "^60.0.0",
    "it-to-stream": "^1.0.0",
    "loglevel": "^1.8.1",
    "loopback-connector-mongodb": "^5.2.3",
    "loopback4-ratelimiter": "^4.2.0",
    "mobile-detect": "^1.4.5",
    "node-telegram-bot-api": "^0.64.0",
    "numeral": "^2.0.6",
    "redis": "^4.6.7",
    "tslib": "^2.0.0",
    "winston": "^3.9.0",
    "x-hub-signature": "^2.1.2"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.3",
    "@loopback/eslint-config": "^13.0.3",
    "@loopback/testlab": "^5.0.3",
    "@types/node": "^14.18.26",
    "eslint": "^8.22.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.7.4"
  }
}
